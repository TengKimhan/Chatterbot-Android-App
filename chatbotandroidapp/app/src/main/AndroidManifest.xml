<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Kimhan.chatbotapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatBotApp">
        <activity android:name="com.Kimhan.chatbotapp.ProfileActivity"></activity>
        <activity android:name="com.Kimhan.chatbotapp.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Kimhan.chatbotapp.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Kimhan.chatbotapp.SignUpActivity" />
        <activity
            android:name="com.Kimhan.chatbotapp.MainActivity"
            android:theme="@style/Theme.StatusBar" />
    </application>

</manifest>